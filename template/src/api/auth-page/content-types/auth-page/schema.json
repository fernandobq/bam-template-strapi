{
  "kind": "singleType",
  "collectionName": "auth_pages",
  "info": {
    "singularName": "auth-page",
    "pluralName": "auth-pages",
    "displayName": "Auth Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "loginTitle": {
      "type": "text"
    },
    "forgotPasswordTitle": {
      "type": "text"
    },
    "forgotPasswordSuccessRequest": {
      "type": "text"
    },
    "resetPasswordTitle": {
      "type": "text"
    },
    "resetPasswordSuccessRequest": {
      "type": "text"
    },
    "backgroundColor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::color.color"
    },
    "authProvider": {
      "type": "enumeration",
      "enum": [
        "local",
        "google",
        "onelogin",
        "github",
        "facebook",
        "twitter"
      ]
    },
    "loginImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
